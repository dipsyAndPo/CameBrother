    where tId = #{tid,jdbcType=INTEGER}
  </delete>
  <insert id="insert" keyColumn="tId" keyProperty="tid" parameterType="cn.source.pojo.Tarde" useGeneratedKeys="true">
    insert into tarde (tName)
    values (#{tname,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" keyColumn="tId" keyProperty="tid" parameterType="cn.source.pojo.Tarde" useGeneratedKeys="true">
    insert into tarde
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="tname != null">
        tName,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="tname != null">
        #{tname,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="cn.source.pojo.Tarde">
    update tarde
